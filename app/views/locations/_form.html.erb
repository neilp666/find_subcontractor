<%= form_for @location do |f| %>

    <%= f.label :company_name %>
    <%= f.text_field :company_name %>

    <%= f.label :address %>
    <%= f.text_field :address %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :postcode %>
    <%= f.text_field :postcode %>


  <%= f.submit 'Add!' %>
<% end %>
